    "@types/jest": "^29.1.1",
    "@types/node": "20.9",
    "eslint": "^7.10.0",
    "jest": "^29.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^4.7.4"
  },
  "optionalDependencies": {
    "https-proxy-agent": "^6.1.0",
    "ts-loader": "^8.0.11",
